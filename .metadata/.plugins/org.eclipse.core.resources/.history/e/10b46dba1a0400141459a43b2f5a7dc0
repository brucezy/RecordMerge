package com.veeva.record;

import java.util.ArrayList;

public class RecordModle {
	
	public ArrayList<String> recordHeader;
	public ArrayList<ArrayList<String>> recordData;
	
	public RecordModle(){
		recordHeader = new ArrayList<String>();
		recordData = new ArrayList<ArrayList<String>>();
	}
	
	public void sort(){
		
	}

	public ArrayList<String> getRecordHeader() {
		return recordHeader;
	}

	public void setRecordHeader(ArrayList<String> recordHeader) {
		this.recordHeader = recordHeader;
	}

	public ArrayList<ArrayList<String>> getRecordData() {
		return recordData;
	}

	public void setRecordData(ArrayList<ArrayList<String>> recordData) {
		this.recordData = recordData;
	}

}
